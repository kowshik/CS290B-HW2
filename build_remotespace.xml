<project name="CS290B-HW2" basedir=".">
    
    <target name="runRemoteSpace" description="runs space on the remote host">
        
        <!-- run remote computer -->
        <java classname="system.SpaceImpl" fork="true">
            <jvmarg value="-Djava.security.policy=policy/policy"/>
